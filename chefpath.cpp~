#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	scanf("%d",&t);
	while(t--)
	{
	long long n,m;	
	scanf("%lld%lld",&n,&m);
	if(m==1 &&n==1)
	printf("No\n");
	if(m<=2 && n<=2)
	printf("Yes\n");
	else if(m==1||n==1)
	printf("No\n");
	else if(m%2==0||n%2==0)
	printf("Yes\n");
	else printf("No\n");
	}
return 0;
}
